Fibonacci series
#include <stdio.h>
int main(){
    int i;
    int j;
    int sum;
    int terms;
    printf("Enter the number of terms:\n");
    scanf("%d",terms);
    
    i = 0;
    j = 1;
    for(int l = 0;l<terms;l++){
        printf("%d",i);
        sum = i + j;
        i = j;
        j = sum;
    }
    
    
    
    return 0;
}

Calculating square of number even or odd
#include <stdio.h>
int main(){
    
    int x;
    
    printf("Enter a positive number:");
    scanf("%d",&x);
    
    if(x>0){
        goto check;
    }
    else{
        goto escape;
    }
    
    check:
        if((x*x)%2==0){
            printf("%d is a even number",x*x);
        }
        else{
            printf("%d is a odd number",x*x);
        }
        return 0;
    
    escape:
        printf("Enter a valid positive number");
        return 0;
    return 0;
}

